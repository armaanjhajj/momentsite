{
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "has": [{ "type": "host", "value": "jobs.havemoments.com" }], "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
